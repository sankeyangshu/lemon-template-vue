{
  "name": "lemon-template-vue",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "description": "An mobile web apps template based on the Vue 3 ecosystem",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/lemon-template-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/lemon-template-vue.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/lemon-template-vue/issues"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0",
    "pnpm": ">=8.15.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:test": "vite build --mode test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "preview": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@unocss/reset": "^0.65.2",
    "@vueuse/core": "^12.2.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vant": "^4.9.15",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify/vue": "^4.2.0",
    "@sankeyangshu/eslint-config": "^0.2.2",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.10.2",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^0.65.1",
    "@unocss/preset-rem-to-px": "^0.65.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "lint-staged": "^15.2.11",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.49",
    "postcss-mobile-forever": "^4.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.6.3",
    "unocss": "^0.65.2",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock-dev-server": "^1.8.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
