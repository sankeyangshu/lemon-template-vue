{
  "name": "lemon-template-vue",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "description": "An mobile web apps template based on the Vue 3 ecosystem",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/lemon-template-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/lemon-template-vue.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/lemon-template-vue/issues"
  },
  "keywords": [
    "vue",
    "templates",
    "echarts",
    "typescript",
    "vant",
    "vueuse",
    "pinia",
    "i18n",
    "unocss"
  ],
  "engines": {
    "node": "^18.18.0 || >=20.0.0",
    "pnpm": ">=8.15.4"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:test": "vite build --mode test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "preview": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "release": "bumpp",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@unocss/reset": "^66.0.0",
    "@vueuse/core": "^12.7.0",
    "axios": "^1.8.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vant": "^4.9.17",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@iconify/vue": "^4.3.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@sankeyangshu/eslint-config": "^0.2.2",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.13.9",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/preset-rem-to-px": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "lint-staged": "^15.4.3",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.3",
    "postcss-mobile-forever": "^4.4.0",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.8.2",
    "unocss": "^66.0.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock-dev-server": "^1.8.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^3.0.7",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
